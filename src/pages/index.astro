<html>
  <head>
    <script>
      const supported = ['en', 'es'];
      const browserLang = (navigator.language || 'en').split('-')[0];
      const lang = supported.includes(browserLang) ? browserLang : 'en';
      window.location.replace(`/${lang}/`);
    </script>
  </head>
  <body></body>
</html>